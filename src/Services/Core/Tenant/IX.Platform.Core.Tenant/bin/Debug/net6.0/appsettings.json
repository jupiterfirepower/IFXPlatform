{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=localhost,1439;Initial Catalog=ixpdevdb;User Id=sa;Password=mssql-password123;"
  },
  "Services": {
    "ServiceName": "TenantService",
    "ServiceVersion": "v0.1"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureServices": {
    "AzureMonitorTrace": "InstrumentationKey=00000000-0000-0000-0000-000000000000;" //your instrumentation key
  },
  "AzureLogging": {
    "FileName": "azure-diagnostics-",
    "FileSizeLimit": 50024,
    "RetainedFileCountLimit": 5
  },
  "DistributedTracing": {
    "IsEnabled": true,
    "Zipkin": {
      "Uri": "http://host.docker.internal:9411/api/v2/spans"
    }
  },
  "HealthChecksUI": {
    "HealthChecksUIEnabled": true,
    "HealthChecks": [
      {
        "Name": "TenantService",
        "Uri": "/health"
      }
    ],
    "ApiMaxActiveRequests": 3,
    "MaximumHistoryEntriesPerEndpoint": 5,
    "EvaluationTimeInSeconds": 180,
    "MinimumSecondsBetweenFailureNotifications": 60
  }
}